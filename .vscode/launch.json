    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: File corrente",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: DTRPO",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/run_dtrpo.py",
            "console": "integratedTerminal",
            "args": [
                "--mode=train",
                "--env=Pendulum",
                "--delay=3",
                "--delta=0.001",
                "--pretrain_epochs=2",
                "--pretrain_steps=250",
                "--max_ep_len=250",
                "--steps_per_epoch=250",
                "--epochs=500",
                "--enc_lr=0.005",
                "--force_stoch_env",
                "--enc_causal",
                "--batch_size_pred=250",
            ]
        },
        ,
        {
            "name": "Python: DDQN",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/run_ddqn.py",
            "console": "integratedTerminal",
            "args": [
                "--mode=train",
                "--env=PuddleWorld",
                // "--seed=2",
                "--delay=3",
                "--learning_rate=0.005",
                "--batch_size=64",
                "--frame_history_len=1",
                "--dueling_dqn",
                "--double_dqn",
                "--learning_freq=1",
                "--q_fun_neurons=64",
                "--enc_causal",
                "--max_timesteps=2000000",
                "--batch_size_pred=64"

            ]
        },
    ]
}